                <div class="tab-pane active" id="pills-upload" role="tabpanel" aria-labelledby="pills-upload-tab">
                    <div class="UppyDashboard" id="uploaderUppyDashboard"></div>
                    <script>
                        const uppy = Uppy.Core({
                            debug: false,
                            autoProceed: false,
                            restrictions: {
                                maxFileSize: 50000000,
                                maxNumberOfFiles: 50,
                                minNumberOfFiles: 1
                                // allowedFileTypes: ['image/*', 'text/*', 'application/*']
                            }
                        })
                        .use(Uppy.Dashboard, {
                            trigger: '.UppyModalOpenerBtn',
                            inline: true,
                            target: '#uploaderUppyDashboard',
                            replaceTargetContent: true,
                            showLinkToFileUploadResult: false,
                            note: 'Upload one file at a time (up to 50 MB)',
                            maxHeight: 450,
                            metaFields: [
                                { id: 'caption', name: 'Caption', placeholder: 'describe what the file is about' }
                            ]
                        })
                        .use(Uppy.XHRUpload, {
                            endpoint: '/uploads.json',
                            method: 'put',
                            formData: true,
                            bundle: false,
                            headers: { 'X-CSRF-Token': $('meta[name="csrf-token"]').attr('content') }
                        })
                        .run()
                        .on('upload-success', (file, body) => {
                            fileId = file.id
                            liID = "#uppy_"+fileId.replace("/", "\\/")
                            liImgWrapEl = $(liID + " .uppy-DashboardItem-previewInnerWrap")

                            divClickImg = $('<div class="uppy-new-upload-img-layer">')
                            divClickImg.append($('<i class="fa fa-plus-circle"></i>'))
                            divClickImg.data('s3_path', body.s3_path)
                            divClickImg.data('filetype', body.filetype)
                            divClickImg.data('filename', body.filename)
                            liImgWrapEl.append(divClickImg)

                            divClickImg.click(function(e) {
                                s3Path = $(this).data('s3_path')
                                filetype = $(this).data('filetype')
                                filename = $(this).data('filename')
                                $(document).trigger('addFileToMarkdownEvent', [filename, filetype, s3Path])
                            });

                            $(document).trigger('refreshGalleryEvent')
                        })

                        $(document).on('upload-reset-modal', (e) => {
                            uppy.reset()
                        })
                    </script>
